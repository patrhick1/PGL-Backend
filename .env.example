# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-2
AWS_S3_BUCKET_NAME=your-s3-bucket-name

# Database Configuration
PGHOST=your_db_host
PGPORT=5432
PGDATABASE=your_db_name
PGUSER=your_db_user
PGPASSWORD=your_db_password

# API Keys
GEMINI_API_KEY=your_gemini_key
OPENAI_API=your_openai_key
ANTHROPIC_API=your_anthropic_key

# FFmpeg Configuration (Windows paths)
FFMPEG_CUSTOM_PATH=C:\ffmpeg\bin\ffmpeg.exe
FFPROBE_CUSTOM_PATH=C:\ffmpeg\bin\ffprobe.exe

# Session Security (REQUIRED for production)
SESSION_SECRET_KEY=your_secure_64_character_random_string_here_generate_new_one

# Server Configuration
PORT=8000
FRONTEND_ORIGIN=http://localhost:5173
IS_PRODUCTION=false


STRIPE_SECRET_KEY=sk_live_xxx
STRIPE_PUBLISHABLE_KEY=pk_live_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx

# Stripe Price IDs
STRIPE_PRICE_FREE=price_free
STRIPE_PRICE_BASIC_MONTHLY=price_xxx
STRIPE_PRICE_PREMIUM_MONTHLY=price_yyy
STRIPE_PRICE_BASIC_YEARLY=price_zzz

# OAuth Configuration
# Google OAuth - Get these from https://console.cloud.google.com
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# OAuth Security
TOKEN_ENCRYPTION_KEY=generate_32_byte_key_here_for_token_encryption
OAUTH_STATE_SECRET=generate_random_secret_here_for_state_validation

# Backend URL (for OAuth callbacks)
BACKEND_URL=http://localhost:8000

# BookingAssistant Integration
# URL for the BookingAssistant API service (required for inbox AI features)
