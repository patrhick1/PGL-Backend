# Nylas Email Integration
NYLAS_API_KEY=your_nylas_api_key_here
NYLAS_API_URI=https://api.us.nylas.com
NYLAS_GRANT_ID=your_nylas_grant_id_here
NYLAS_WEBHOOK_SECRET=your_webhook_secret_here

# Email Provider Configuration
DEFAULT_EMAIL_PROVIDER=nylas
EMAIL_SEND_RATE_LIMIT=700
EMAIL_SEND_HOURLY_LIMIT=50