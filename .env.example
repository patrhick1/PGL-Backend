# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-2
AWS_S3_BUCKET_NAME=your-s3-bucket-name

# Database Configuration
PGHOST=your_db_host
PGPORT=5432
PGDATABASE=your_db_name
PGUSER=your_db_user
PGPASSWORD=your_db_password

# API Keys
GEMINI_API_KEY=your_gemini_key
OPENAI_API=your_openai_key
ANTHROPIC_API=your_anthropic_key

# FFmpeg Configuration (Windows paths)
FFMPEG_CUSTOM_PATH=C:\ffmpeg\bin\ffmpeg.exe
FFPROBE_CUSTOM_PATH=C:\ffmpeg\bin\ffprobe.exe

# Session Security (REQUIRED for production)
SESSION_SECRET_KEY=your_secure_64_character_random_string_here_generate_new_one

# Server Configuration
PORT=8000
FRONTEND_ORIGIN=http://localhost:5173
IS_PRODUCTION=false